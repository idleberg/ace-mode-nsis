{
  "name": "ace-nsis-mode",
  "description": "Ace mode for NSIS (Nullsoft Scriptable Install System)",
  "version": "1.5.3",
  "author": "Jan T. Sott",
  "license": "BSD-3-Clause",
  "browser": "./dist/mode-nsis.noconflict.js",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "bootstrap": "node ./tools/bootstrap.mjs",
    "build": "node ./tools/build.mjs",
    "dev": "npm run start",
    "fix": "eslint --fix ./src",
    "lint": "eslint ./src",
    "prepare": "husky install",
    "start": "rollup --watch --config",
    "serve": "sirv -B ./",
    "test": "npm run lint"
  },
  "keywords": [
    "ace",
    "mode",
    "nsis",
    "nullsoft",
    "syntax highlighter"
  ],
  "homepage": "https://github.com/idleberg/ace-nsis-mode",
  "repository": "https://github.com/idleberg/ace-nsis-mode",
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-recommended": "^4.1.0",
    "husky": "^8.0.1",
    "latest-version": "^7.0.0",
    "lint-staged": "^13.0.3",
    "shelljs": "^0.8.5",
    "sirv-cli": "^2.0.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
